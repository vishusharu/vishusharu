package abhi;

import java.util.Scanner;

public class Ghhhjjh {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Scanner sc= new Scanner (System.in);
	int num=0;
	System.out.println("1:string to int convert \n"+
			"2:string to float convert\n"+"3:string to double convert\n"+"4:string to byte datatype convert\n"+"5:string to short convert\n"+"2:string to long convert\n");

	
	num=nextInt();
	switch(num) {
	
	case 1:
String a = "487";
Integer a1= new Integer(a);
System.out.println(a1+a1);
break;
// get and print their absolute values

	case 2:
String b = "677";
Float b1= new Float(b);
System.out.println(b1);

break;

	case 3:
String c = "487";
Double c1= new Double(c);
System.out.println(c1);
break;

	case 4:
String d = "487";
Byte  d1= new Byte(d);
System.out.println(d1);
 break;
 
	case 5:
		
String e = "487";
Short e1= new Short(e);
System.out.println(e1);
 break;

	
	case 6:
	
String f = "487";
Long f1= new Long(f);
System.out.println(f1);
 break;
  
	case 7:
		System.exit(0);
  
	default :
			//System.out.println("enter your correct choice");
             break;


	}
		}

	private static int nextInt() {
		// TODO Auto-generated method stub
		return 0;
	}
	
}	

	
	


